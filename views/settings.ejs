<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Your setings</title>
  </head>
  <body>
    <%- include('./navbar.ejs') -%>
    <main class="container">
      <% if (status == '2') { %>
      <div class="row table-responsive">
        <table class="table table-hover table-dark">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Username</th>
              <th scope="col">Email</th>
              <th scope="col">Status</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i < adminInfo.length; i++) { %>
            <tr>
              <th scope="row"><%= i %></th>
              <td><%= adminInfo.username[i] %></td>
              <td><%= adminInfo.email[i] %></td>
              <% if (adminInfo.status[i] == '1') { %>
                <td>User</td>
              <% } else if (adminInfo.status[i] == '2'){ %>
                <td>Admin</td>
              <% } %>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      <% } if (status == '1') { %>
      <div class="row">

      </div>
      <div class="row">

      </div>
      <% } %>
    </main>
    <h1>test <%= test %></h1>

    <%- include('./footer.ejs') -%>
  </body>
</html>
